<Window x:Class="TuringTrader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TuringTrader"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="AlgoSelector" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="282" Loaded="AlgoSelector_Loaded" SelectionChanged="AlgoSelector_SelectionChanged"/>
        <Button x:Name="RunButton" Content="Run" HorizontalAlignment="Left" Margin="612,10,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="RunButton_Click"/>
        <Button x:Name="ReportButton" Content="Report" HorizontalAlignment="Left" Margin="707,10,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="ReportButton_Click"/>
        <TextBox x:Name="LogOutput" HorizontalAlignment="Left" Height="362" VerticalScrollBarVisibility="Auto" Margin="10,47,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="772" Loaded="LogOutput_Loaded" TextChanged="LogOutput_TextChanged"/>
        <!-- <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Left" Height="361" Margin="10,48,0,0" VerticalAlignment="Top" Width="772" >
            <TextBox x:Name="LogOutput" TextWrapping="Wrap" Text="" Loaded="LogOutput_Loaded" TextChanged="LogOutput_TextChanged"/>
        </ScrollViewer> -->

    </Grid>
</Window>
