<Window x:Class="TuringTrader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TuringTrader"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="AlgoSelector" HorizontalAlignment="Left" Margin="239,51,0,0" VerticalAlignment="Top" Width="169" Loaded="AlgoSelector_Loaded" SelectionChanged="AlgoSelector_SelectionChanged"/>
        <Button x:Name="RunButton" Content="Run" HorizontalAlignment="Left" Margin="333,98,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="RunButton_Click"/>
        <Button x:Name="ReportButton" Content="Report" HorizontalAlignment="Left" Margin="333,143,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="ReportButton_Click"/>

    </Grid>
</Window>
